<div class="container">
  <% Like.all.each do |like| %>
  <%if like.user_id == current_user.id%>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">
            <a href="/users/<%=like.user_id%>"><%=like.user.username%></a>
            <small>
              <%=time_ago_in_words(like.photo.created_at)%>
            </small>
          </h3>
        </div>

        <div class="panel-body">
          <a href="/photos/<%=like.photo.id%>">
            <img src="<%=like.photo.image%>" class="img-responsive"> </a>
            <a> <%= like.photo.caption %> </a>
            <% Comment.all.each do |comment| %>
            <%if comment.photo_id == like.photo.id%>
            <li class="media">
              <div class="media-body">
                <h6 class="media-heading">
                  <a href="/users/144"><%=comment.user.username%></a>
                  <small>
                    <%=time_ago_in_words(comment.created_at)%>
                  </small>
                </h6>
                <%=comment.body%>
              </div>
            </li>
            <%end%>
            <%end%>

            <p>
              <a href="/delete_like/<%=like.id%>" class="btn btn-link">
                <i class="fa fa-heart"></i>
              </a>
              <%=current_user.username%>
              likes
              this
            </p>
          </div>
        </div>
      </div>
    </div>
    <% end %>
    <% end %>
  </div>
