<div class="page-header">
  <h1>
    My Likes
  </h1>
</div>

<div>
  <table class="table table-striped table-hover">
    <tr>
      <th>Photos You Liked</th>
    </tr>

    <% Like.all.each do |like| %>
    <tr>
      <% if like.user_id == current_user.id %>
      <td><img src = "<%= like.photo.image %>"></td>
      <% end %>
    </tr>
    <% end %>
  </table>
</div>
